<div class="education mt-6">
  <div class="title title-content">Experience</div>
  <div class="timeline">
    <header class="timeline-header">
      <span class="tag is-medium is-primary">Start</span>
    </header>
    <%exps.each do |exp|%>
    <div class="timeline-item">
      <div class="timeline-marker"></div>
      <div class="timeline-content">
        <p class="heading"> <%=time_tag exp.start_date,:format=>'%b %d,%Y'%></p>
        <%=exp.name%>
      </div>
    </div>
    <header class="timeline-header">
      <span class="tag is-primary">
        <%=time_tag exp.end_date,:format=>'%Y'%>
      </span>
    </header>
    <%end%>
  </div>
</div>
 
